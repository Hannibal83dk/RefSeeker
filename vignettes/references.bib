journal = {Oncology Letters},
keywords = {MicroRNA-23a,MicroRNA-23b,Ovarian cancer,Prognosis},
month = {jul},
number = {1},
pages = {262--266},
pmid = {29928410},
title = {{Correlation analysis on the expression levels of microRNA-23a and microRNA-23b and the incidence and prognosis of ovarian cancer}},
url = {http://www.spandidos-publications.com/10.3892/ol.2018.8669 http://www.ncbi.nlm.nih.gov/pubmed/29928410 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6006491},
volume = {16},
