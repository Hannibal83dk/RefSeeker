doi = {10.1016/j.ymthe.2021.03.015},
file = {:C\:/Users/PPET0123/Desktop/Arikler/Bioinspired artificial exosomes based on lipid nanoparticles carrying let-7b-5p promote angiogenesis in vitro and in vivo.pdf:pdf},
