issn = {18767958},
journal = {Neural Regeneration Research},
keywords = {Internal control genes,NormFinder analysis,Normalization,Reference genes,Reverse transcription-quantitative polymerase chai,Spinal cord injury,Stability of gene expression,geNorm analysis},
month = {jun},
number = {6},
pages = {1387--1392},
pmid = {34782586},
title = {{Selection of suitable internal controls for gene expression normalization in rats with spinal cord injury}},
url = {https://journals.lww.com/10.4103/1673-5374.327350 http://www.ncbi.nlm.nih.gov/pubmed/34782586 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC8643046},
