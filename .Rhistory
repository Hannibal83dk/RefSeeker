rs_ktable2b <- function(refseekerlist){
library(flextable)
library(officer)
table <- data.frame( matrix(nrow = nrow(refseekerlist[[1]]), ncol = 11 ) )
table[1] <- refseekerlist[[1]][1]
for(i in 1:5){                    #       (ncol(results$Fresh_Frozen$stabilityTable))){
