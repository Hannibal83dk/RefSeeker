#'
#' @return the absolute path to a file
#'
#' @examples
#'
#' \dontrun{
#' absfilepath("../path/to/file")
#' }
#'
#'
absfilepath <- function(relativepath) {
path <- paste(
normalizePath(dirname(relativepath)),
basename(relativepath),
sep = "/"
)
return(path)
}
